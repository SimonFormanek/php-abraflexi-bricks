{
    "name": "vitexsoftware/flexibee-bricks",
    "description": "php-flexibee extending classes and widgets",
    "type": "library",
    "authors": [
        {
            "name": "Vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "vitexsoftware/ease-html": "^1.23",
        "spojenet/flexibee": "^1.37"
    },
    "autoload": {
        "psr-4": {
            "FlexiPeeHP\\Bricks\\": "src/FlexiPeeHP/Bricks",
            "FlexiPeeHP\\Bricks\\ConvertRules\\": "src/FlexiPeeHP/Bricks/ConvertRules",
            "FlexiPeeHP\\ui\\": "src/FlexiPeeHP/ui"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": "vendor/vitexsoftware/ease-core/tests/src/Ease",
            "Test\\FlexiPeeHP\\": "vendor/spoje.net/flexibee/testing/src/FlexiPeeHP/",
            "Test\\FlexiPeeHP\\ui\\": "testing/FlexiPeeHP/ui",
            "Test\\FlexiPeeHP\\Bricks\\": "testing/FlexiPeeHP/Bricks",
            "Test\\": "testing/"
        }
    },
    "require-dev": {
        "codeception/codeception": "*",
        "phpunit/phpunit": "*"
    }
}
